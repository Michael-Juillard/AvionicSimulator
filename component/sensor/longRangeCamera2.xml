<?xml version="1.0" encoding="UTF-8"?>
<sensor>
	<name>longRangeCamera2</name>
	<type>camera</type>
	<initalMode>1</initalMode>
    <!-- resolution 1280x1280 with 16 bits => 26.2 Mb -->
	<modes>
		<mode id="1">
			<name>IDL</name>
			<power unit="mW">10</power>
			<packetSize unit="Mbits">26.2</packetSize>
			<ratePacket unit="ms">400</ratePacket>
			<accuracySensor>0.0</accuracySensor>
		</mode>
		<mode id="2">
			<name>TargetFinding</name>
			<power unit="mW">180</power>
			<packetSize unit="Mbits">26.2</packetSize>
			<ratePacket unit="ms">11</ratePacket>
			<accuracySensor>0.3</accuracySensor>
		</mode>
        <mode id="3">
			<name>TargetTracking</name>
			<power unit="mW">120</power>
			<packetSize unit="Mbits">26.2</packetSize>
			<ratePacket unit="ms">30</ratePacket>
			<accuracySensor>0.4</accuracySensor>
		</mode>
	</modes>
</sensor>
